{
  "name": "cuban-social",
  "version": "0.4.0",
  "description": "Cuban Social - Discover the best Cuban dance events in San Diego",
  "type": "module",
  "main": "index.html",
  "scripts": {
    "start": "python3 -m http.server 8000",
    "export-events": "node scripts/supabase-to-json.js",
    "insert-missing-events": "python3 scripts/insert-missing-events.py",
    "insert-missing-dry-run": "python3 scripts/insert-missing-events.py --dry-run",
    "insert-missing-force": "python3 scripts/insert-missing-events.py --force",
    "generate-cards": "node scripts/generate-event-cards.js",
    "list-cards": "node scripts/list-cards.js",
    "json-to-csv": "python3 scripts/json_to_csv.py",
    "compare-data": "python3 scripts/compare-csv.py",
    "compare-data-verbose": "python3 scripts/compare-csv.py --verbose"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/johandry/CubanSocial.git"
  },
  "keywords": [
    "salsa",
    "timba",
    "bachata",
    "merengue",
    "cuban-dance",
    "san-diego",
    "events",
    "community",
    "github-pages"
  ],
  "author": "Cuban Social Community",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johandry/CubanSocial/issues"
  },
  "homepage": "https://cubansocial.com",
  "devDependencies": {
    "gh-pages": "^6.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.54.0",
    "canvas": "^3.1.2"
  }
}
